AddDefaultCharset UTF-8
SetEnv PYTHONIOENCODING utf8
Options FollowSymLinks ExecCGI
RewriteEngine On

RewriteRule ^api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=main
RewriteRule ^catalogs/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=catalogs
RewriteRule ^taxonomy/term/(.*?),(.*?)/api/p <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=taxonomy&marketId=$1&categoryId=$2
RewriteRule ^featured/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=list&type=featured
RewriteRule ^featured/(.*?)/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=list&type=featured&marketId=$1
RewriteRule ^recent/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=list&type=recent
RewriteRule ^favorites/top/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=list&type=favorites
RewriteRule ^popular/top/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=list&type=popular
RewriteRule ^node/(.*?)/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=content&plugId=$1
RewriteRule ^api/p/search/apachesolr_search/(.*?)$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=search&query=$1
RewriteRule ^content/(.*?)/api/p$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=content&plugId=$1
RewriteRule ^content/(.*?)$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=redirect&plugId=$1

ReWriteRule ^$ <INSERT URL HERE>/cgi-bin/msInterface.cgi?action=goto_wiki

